{
  "version": 2,
  "name": "intern-management-app-nwannaji",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://intern-management-backend-gi46.onrender.com/api",
    "VITE_MEDIA_URL": "https://intern-management-backend-gi46.onrender.com/media"
  }
}
